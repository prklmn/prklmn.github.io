<!-- Typed.js -->
<script src="js/jquery-1.11.2.min.js"></script>
<script src="js/typed.js" type="text/javascript"></script>
<script>
  $(function(){
    $(".typed").typed({
      strings: ["ddRAD.","ezRAD.", "SE-RAD.", "PE-RAD.","Reference-based RAD."],
      typeSpeed: 100,
      loop: true,
      backDelay: 1000
    });
  });
</script>

<div class="row">
  <div class="col-sm-12">
      <div class="text-center">
